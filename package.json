{
  "name": "react-recurring",
  "version": "0.0.5",
  "description": "A recurring date selection widget for React.",
  "repository": {
    "type": "git",
    "url": "https://github.com/ABASystems/react-recurring.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "build:prod": "webpack --config webpack.config.production.js --progress --colors",
    "build": "webpack --config webpack.config.base.js --progress --colors",
    "watch": "node server.js",
    "test": "mocha --require mock-css-modules --harmony-proxies --compilers js:babel-register -R nyan",
    "prepublish": "npm run build:prod"
  },
  "author": "Luke Hodkinson",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-css-modules-transform": "0.0.7",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.5.2",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "jsdom": "^8.1.0",
    "jsdom-global": "^1.7.0",
    "mocha": "^2.4.5",
    "mock-css-modules": "^1.0.0",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-node-externals": "^1.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "react": "^0.13.3",
    "rrule": "git+https://github.com/jkbrzt/rrule.git"
  }
}
